from django.conf.urls.defaults import patterns, url, include

urlpatterns = patterns('billing_record.views',
   #web


   url(r'^$', 'index'),
   url(r'^(?P<year>\d{4})(?:/(?P<pdf>pdf))?/$', 'index'),
   url(r'^(?P<month>\d{1,2})/$', 'index'),
   url(r'^(?P<year>\d{4})/(?P<month>\d{1,2})/$', 'index'),
   url(r'^(?P<year>\d{4})/(?P<month>\d{1,2})/(?P<expense_code>[-\d]{39})/$', 'index'),
   url(r'^(?P<year>\d{4})/(?P<month>\d{1,2})/(?P<ec_root>\d{5})/$', 'index'),
   #pdf

)
